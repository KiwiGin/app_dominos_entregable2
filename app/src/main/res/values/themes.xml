<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.App_figma1" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.App_figma1.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.App_figma1.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.App_figma1.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="MenuTextStyle">
        <item name="android:textSize">32sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/alumnisanssemibold</item>
    </style>

    <style name="MenuLogin">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/alumnisanssemibold</item>
    </style>

    <style name="t1">
        <item name="android:textColor">#006491</item>
        <item name="android:fontFamily">@font/alumnisanssemibold</item>
    </style>

    <style name="t2">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/alumnisanssemibold</item>
    </style>
</resources>